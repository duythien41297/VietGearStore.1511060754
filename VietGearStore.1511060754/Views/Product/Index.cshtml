
@{
    ViewBag.Title = Model.name;
    Layout = "~/Views/Shared/Layout.cshtml";
}
<div class="main">
    <div class="content">
        <div class="content_top">
            <div class="heading">
                <h3>@Model.name</h3>
            </div>
            <div class="clear"></div>
        </div>
        <div class="section group">
            @Html.Action("getProduct", "Default", new { id = Model.id, metatitle =@Model.meta })
        </div>
    </div>
 </div>
            
