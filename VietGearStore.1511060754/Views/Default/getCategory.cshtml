@model List<VietGearStore._1511060754.Models.Category>

@foreach (var i in Model)
{
<div class="heading">
    <h3><a href="@ViewBag.meta/@i.meta/">@i.name</a></h3>
</div>
    <div class="section group">
        @Html.Action("getProduct", "Default", new { id = @i.id, metatitle = @ViewBag.metatitle })
    </div>
}
